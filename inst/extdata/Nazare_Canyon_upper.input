################################ VARIABLES 
!-- DETRITUS
 lDetDep                 =   lDet_w->deadlDet
 sDetDep                 =   sDet_w->deadsDet
 rDetDep                 =   rDet_w->deadrDet
 lDetDis                 =   deadlDet->deadDOC
 sDetDis                 =   deadsDet->deadDOC
 rDetDis                 =   deadrDet->deadDOC
 lDetUpt                 =   flowfrom(deadlDet) - lDetDis
 sDetUpt                 =   flowfrom(deadsDet) - sDetDis
 DetDep                  =   lDetDep + sDetDep + rDetDep
 DetDis                  =   lDetDis + sDetDis + rDetDis
 BurialOC                =   deadrDet->Burial

!-- deadDOC
 DOCefflux               =   deadDOC->DOC_w

!-- BACTERIA
 BacResp                =   Bac->DIC
 BacUpt                 =   deadDOC->Bac
 BacDocProd             =   Bac->deadDOC
 BacProd                =   BacUpt - BacResp
 BacMort = BacDocProd ! added for tutorial
 BacAss = BacUpt ! added for tutorial


!-- MEIOBENTHOS
 MeiSFResp              =   MeiSF->DIC
 MeiSFDetProd           =   MeiSFsDetProd + MeiSFrDetProd
 MeiSFUptLab            =   deadlDet->MeiSF   +  Bac->MeiSF
 MeiSFUptSem            =   deadsDet->MeiSF
 MeiSFUpt               =   flowto(MeiSF)
 MeiSFAssLab            =   MeiSFUptLab - MeiSFsDetProd
 MeiSFAssSem            =   MeiSFUptSem - MeiSFrDetProd
 MeiSFAss               =   MeiSFAssLab + MeiSFAssSem
 MeiSFProd              =   MeiSFAss    - MeiSFResp
 lDetUptMeiSF           =   deadlDet->MeiSF
 sDetUptMeiSF           =   deadsDet->MeiSF
 BacUptMeiSF            =   Bac->MeiSF

 MeiNFResp              =   MeiNF->DIC
 MeiNFDetProd           =   MeiNFsDetProd + MeiNFrDetProd
 MeiNFUptLab            =   deadlDet->MeiNF   +  Bac->MeiNF
 MeiNFUptSem            =   deadsDet->MeiNF
 MeiNFUpt               =   flowto(MeiNF)
 MeiNFAssLab            =   MeiNFUptLab - MeiNFsDetProd
 MeiNFAssSem            =   MeiNFUptSem - MeiNFrDetProd
 MeiNFAss               =   MeiNFAssLab + MeiNFAssSem
 MeiNFProd              =   MeiNFAss    - MeiNFResp
 MeiNFMort              =   MeiNF->deadlDet
 lDetUptMeiNF           =   deadlDet->MeiNF
 sDetUptMeiNF           =   deadsDet->MeiNF
 BacUptMeiNF            =   Bac->MeiNF

 MeiPOResp              =   MeiPO->DIC
 MeiPODetProd           =   MeiPOsDetProd + MeiPOrDetProd
 MeiPOUptLab            =   deadlDet->MeiPO   +  Bac->MeiPO + MeiSF->MeiPO + MeiNF->MeiPO
 MeiPOUptSem            =   deadsDet->MeiPO
 MeiPOUptPO             =   MeiSF->MeiPO + MeiNF->MeiPO
 MeiPOUpt               =   flowto(MeiPO)
 MeiPOAssLab            =   MeiPOUptLab - MeiPOsDetProd
 MeiPOAssSem            =   MeiPOUptSem - MeiPOrDetProd
 MeiPOAss               =   MeiPOAssLab + MeiPOAssSem
 MeiPOProd              =   MeiPOAss    - MeiPOResp
 MeiPOMort              =   MeiPO->deadlDet

 MeiResp                =   MeiSFResp + MeiNFResp + MeiPOResp
 MeiProd                =   MeiSFProd + MeiNFProd + MeiPOProd

!-- MACROBENTHOS
 MacSDFResp             =   MacSDF->DIC
 MacSDFDetProd          =   MacSDFsDetProd + MacSDFrDetProd
 MacSDFUptLab           =   deadlDet->MacSDF  +  Bac->MacSDF + MeiSF->MacSDF + MeiNF->MacSDF + MeiPO->MacSDF
 MacSDFUptSem           =   deadsDet->MacSDF
 MacSDFUpt              =   flowto(MacSDF)
 MacSDFAssLab           =   MacSDFUptLab - MacSDFsDetProd
 MacSDFAssSem           =   MacSDFUptSem - MacSDFrDetProd
 MacSDFAss              =   MacSDFAssLab + MacSDFAssSem
 MacSDFProd             =   MacSDFAss - MacSDFResp
 MacSDFMort             =   MacSDF->deadlDet
 MacSDFExp              =   MacSDF->Export

 MacDFResp              =   MacDF->DIC
 MacDFDetProd           =   MacDFsDetProd + MacDFrDetProd
 MacDFUptLab            =   deadlDet->MacDF  +  Bac->MacDF + MeiSF->MacDF + MeiNF->MacDF + MeiPO->MacDF
 MacDFUptSem            =   deadsDet->MacDF
 MacDFUpt               =   flowto(MacDF)
 MacDFAssLab            =   MacDFUptLab - MacDFsDetProd
 MacDFAssSem            =   MacDFUptSem - MacDFrDetProd
 MacDFAss               =   MacDFAssLab + MacDFAssSem
 MacDFProd              =   MacDFAss - MacDFResp
 MacDFMort              =   MacDF->deadlDet
 MacDFExp               =   MacDF->Export

 MacSFResp              =   MacSF->DIC
 MacSFDetProd           =   MacSFsDetProd + MacSFrDetProd
 MacSFUptLab            =   lDet_w->MacSF
 MacSFUptSem            =   sDet_w->MacSF
 MacSFUpt               =   flowto(MacSF)
 MacSFAssLab            =   MacSFUptLab - MacSFsDetProd
 MacSFAssSem            =   MacSFUptSem - MacSFrDetProd
 MacSFAss               =   MacSFAssLab + MacSFAssSem
 MacSFProd              =   MacSFAss - MacSFResp
 MacSFMort              =   MacSF->deadlDet
 MacSFExp               =   MacSF->Export

 MacPSResp              =   MacPS->DIC
 MacPSDetProd           =   MacPSsDetProd + MacPSrDetProd
 MacPSUptLab            =   flowto(MacPS) - deadsDet->MacPS
 MacPSUptSem            =   deadsDet->MacPS
 MacPSUptPS             =   flowto(MacPS) - deadlDet->MacPS - Bac->MacPS - deadsDet->MacPS
 MacPSUpt               =   MacPSUptLab + MacPSUptSem
 MacPSAssLab            =   MacPSUptLab - MacPSsDetProd
 MacPSAssSem            =   MacPSUptSem - MacPSrDetProd
 MacPSAss               =   MacPSAssLab + MacPSAssSem
 MacPSProd              =   MacPSAss - MacPSResp
 MacPSMort              =   MacPS->deadlDet
 MacPSExp               =   MacPS->Export

 MacResp                =   MacSDFResp + MacDFResp + MacSFResp + MacPSResp
 MacProd                =   MacSDFProd + MacDFProd + MacSFProd + MacPSProd
 MacExp                 =   MacSDFExp  + MacDFExp  + MacSFExp  + MacPSExp

 !-- MEGABENTHOS
 MegSDFResp              =   MegSDF->DIC
 MegSDFDetProd           =   MegSDFsDetProd + MegSDFrDetProd
 MegSDFUptLab            =   deadlDet->MegSDF  +  Bac->MegSDF + MeiSF->MegSDF + MeiNF->MegSDF + MeiPO->MegSDF ! MeiPO added by Danielle
 MegSDFUptSem            =   deadsDet->MegSDF
 MegSDFUpt               =   flowto(MegSDF)
 MegSDFAssLab            =   MegSDFUptLab - MegSDFsDetProd
 MegSDFAssSem            =   MegSDFUptSem - MegSDFrDetProd
 MegSDFAss               =   MegSDFAssLab + MegSDFAssSem
 MegSDFProd              =   MegSDFAss - MegSDFResp
 MegSDFMort              =   MegSDF->deadlDet
 MegSDFExp               =   MegSDF->Export

 MegDFResp              =   MegDF->DIC
 MegDFDetProd           =   MegDFsDetProd + MegDFrDetProd
 MegDFUptLab            =   deadlDet->MegDF  +  Bac->MegDF + MeiSF->MegDF + MeiNF->MegDF + MeiPO->MegSDF ! MeiPO added by Danielle
 MegDFUptSem            =   deadsDet->MegDF
 MegDFUpt               =   flowto(MegDF)
 MegDFAssLab            =   MegDFUptLab - MegDFsDetProd
 MegDFAssSem            =   MegDFUptSem - MegDFrDetProd
 MegDFAss               =   MegDFAssLab + MegDFAssSem
 MegDFProd              =   MegDFAss - MegDFResp
 MegDFMort              =   MegDF->deadlDet
 MegDFExp               =   MegDF->Export

 MegSFResp              =   MegSF->DIC
 MegSFDetProd           =   MegSFsDetProd + MegSFrDetProd
 MegSFUptLab            =   lDet_w->MegSF
 MegSFUptSem            =   sDet_w->MegSF
 MegSFUpt               =   flowto(MegSF)
 MegSFAssLab            =   MegSFUptLab - MegSFsDetProd
 MegSFAssSem            =   MegSFUptSem - MegSFrDetProd
 MegSFAss               =   MegSFAssLab + MegSFAssSem
 MegSFProd              =   MegSFAss - MegSFResp
 MegSFMort              =   MegSF->deadlDet
 MegSFExp               =   MegSF->Export

 MegPSResp              =   MegPS->DIC
 MegPSDetProd           =   MegPSsDetProd + MegPSrDetProd
 MegPSUptLab            =   flowto(MegPS) - deadsDet->MegPS
 MegPSUptSem            =   deadsDet->MegPS
 MegPSUptPS             =   flowto(MegPS) - deadlDet->MegPS - Bac->MegPS - deadsDet->MegPS
 MegPSUpt               =   flowto(MegPS)
 MegPSAssLab            =   MegPSUptLab - MegPSsDetProd
 MegPSAssSem            =   MegPSUptSem - MegPSrDetProd
 MegPSAss               =   MegPSAssLab + MegPSAssSem
 MegPSProd              =   MegPSAss - MegPSResp
 MegPSMort              =   MegPS->deadlDet
 MegPSExp               =   MegPS->Export

 MegUpt                 =   MegSDFUpt  + MegDFUpt  + MegSFUpt  + MegPSUpt
 MegResp                =   MegSDFResp + MegDFResp + MegSFResp + MegPSResp
 MegProd                =   MegSDFProd + MegDFProd + MegSFProd + MegPSProd
 MegExp                 =   MegSDFExp  + MegDFExp  + MegSFExp  + MegPSExp

!-- TOTALS
 TotDeposition          =   lDetDep + sDetDep + rDetDep
 TotSuspensionfeeding   =   lDet_w->MacSF + lDet_w->MegSF &
                          + sDet_w->MacSF + sDet_w->MegSF
 TotOCinput             =   TotDeposition + TotSuspensionfeeding
 TotSecProd             =   BacProd + MeiProd + MacProd + MegProd
 TotExport              =   MacExp  + MegExp
 TotResp                =   BacResp + MeiResp + MacResp + MegResp

################################ END VARIABLES


